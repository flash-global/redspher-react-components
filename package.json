{
  "name": "redspher-components",
  "version": "2.0.0",
  "description": "reusable components for redspher",
  "author": "Vincent Racelle",
  "license": "MIT",
  "repository": "yoctu/redspher-react-components",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean && tsc",
    "build": "microbundle-crl --no-compress",
    "start": "npm run clean && tsc && microbundle-crl watch --no-compress",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "npm install && npm --prefix ./example install && npm --prefix ./example run build",
    "deploy": "gh-pages -d example/build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@date-io/moment": "^2.13.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.75",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "@mui/system": "^5.4.0",
    "@mui/x-date-pickers": "^5.0.0",
    "date-fns": "^2.16.1",
    "globalyzer": "^0.1.4",
    "libphonenumber-js": "^1.9.49",
    "material-ui-phone-number": "^3.0.0",
    "moment": "^2.29.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-mui-dropzone": "^4.0.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
